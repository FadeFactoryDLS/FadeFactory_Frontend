{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    { "source": "/register/(.*)", "destination": "/register/" },
    { "source": "/login/(.*)", "destination": "/login/" },
    { "source": "/logout/(.*)", "destination": "/logout" },
    { "source": "/admin/dashboard/(.*)", "destination": "/admin/dashboard/" }
  ]
}
